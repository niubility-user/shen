<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/a5q" android:layout_width="match_parent" android:layout_height="48dp" android:layout_alignParentTop="true">
        <EditText android:textSize="18sp" android:gravity="center" android:id="@+id/a5r" android:layout_width="wrap_content" android:layout_height="48dp" android:hint="输入配置Id" android:singleLine="true"/>
        <TextView android:textSize="18sp" android:gravity="center" android:layout_width="wrap_content" android:layout_height="48dp" android:text="Host配置" android:layout_centerInParent="true"/>
        <CheckBox android:textSize="10sp" android:id="@+id/a5s" android:layout_width="wrap_content" android:layout_height="48dp" android:layout_marginRight="5dp" android:checked="true" android:text="前两项" android:layout_alignParentRight="true"/>
    </RelativeLayout>
